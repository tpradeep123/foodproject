<style>

  .maincontainer{
  height: 100%;
  width: 100vw;
      background-color: #fa8231;
      display: flex;
     justify-content: center;
  align-items: center;
  
  
  font-family: Poppins;
  }
  
  .box{
  width: 80vw;
  height: auto;
  background-color: white;
  border-radius: 15px;
  padding: 20px;
  /*background: transparent;*/
  }
  
  .headingdiv{
  padding: 5px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  
  
  }
</style>    




<html>
  <head>
      <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Poppins&display=swap" rel="stylesheet">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js" integrity="sha384-oBqDVmMz9ATKxIep9tiCxS/Z9fNfEXiDAYTujMAeBAsjFuCZSmKbSSUnQlmh/jp3" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.min.js" integrity="sha384-mQ93GR66B00ZXjt0YO5KlohRA5SY2XofN4zfuZxLkoj1gXtW8ANNCe9d5Y3eG5eD" crossorigin="anonymous"></script>
  
  
  </head>
  <div class="maincontainer">
    
      <div class="box">
          <div class="headingdiv">
              <div>
              <img src="/images/book.png" width="70" height="70" />
              <span style="letter-spacing:1;font-size: 24px;font-weight:bold;margin-left: 10px;">Book List</span>
              </div>
              <div>
                <a href="/foodproject/food" style="text-decoration:none;">
                  <img src="/images/add.png" width="50" height="50" />
                  </a>
              </div>
            </div>
  <table class="table table-striped">
      <thead>
        <tr>
          
          <th scope="col">Dishid</th>
         <th scope="col">Foodtype</th>
          <th scope="col">Fooditem</th>
          <th scope="col">veg/nonveg</th>
          <th scope="col">status</th>
          <th scope="col">discription</th>
          <th scope="col">Price<br>Offer</th>
          <th scope="col">Picture</th>           
          <th scope="col">Edit/Delete</th>


        </tr>
      </thead>

      <tbody>
          <% data.map((item)=>{%>
        <tr>
          <th scope="row"><%=item.dishid %></th>
          <td><%= item.foodtype%></td>
          <td><%= item.fooditem %></td>
          <td><%= item.status1 %></td>
            <td><%= item.status2 %></td>
            <td><%= item.discription %></td>
          <td><s><%= item.price %></s><br><%= item.offer %></td>
         
          
          <td><a href="/foodproject/display_picture?dishid=<%= item.dishid %>&fooditem=<%= item.fooditem %>&picture=<%= item.picture %>"><img src="/images/<%= item.picture %>" style="width:40px;border-radius: 10px;"/></a></td>
            <td><a href='/foodproject/edit_food_data?dishid=<%= item.dishid %>'style='text-decoration:none;font-weight: 700;'>Edit/Delete</a></td>

        </tr>
     <% }) %>
      </tbody>
  
    </table>
    </div>
    </div>
</html>